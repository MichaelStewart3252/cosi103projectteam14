
<% if (!loggedIn) {%> 

    <h1>links to team-members pages</h1>

<% } else if ( APIsaved ) {%> 

    <h2>links to team-members pages</h2>
    <h2>Your apikey is saved! Go try our prompt!</h2>
    <form method="get" action="/deleteAPIKEY">
        <input type="submit" value="delete API key"> 
    </form>

<% } else {%> 

    <h2>links to team-members pages</h2>
    <div class="col-sm-5">
        <% if ( invalidKey ) { %>
            <h1>try again</h1>
        <% } else { %>
            <h1>save your apikey for futher use!</h1>
        <% } %>
        <form method="post" action="/save">
            apikey: 
                <input type="text" autocomplete="apikey" name="apikey"><br>
            confirm apikey: 
                <input type="text" name="apikey2"><br>       
                
                
            <input type="submit">
        </form>
    </div>
    
<% } %>
    
